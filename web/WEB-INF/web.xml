<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>findcategory</servlet-name>
        <servlet-class>find.findcategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>findactor</servlet-name>
        <servlet-class>find.findactor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>trama</servlet-name>
        <servlet-class>find.trama</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>findcategory</servlet-name>
        <url-pattern>/findcategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findactor</servlet-name>
        <url-pattern>/findactor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>trama</servlet-name>
        <url-pattern>/trama</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
